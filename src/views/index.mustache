<!DOCTYPE html>
<html lang="{{ lang.code }}">
<head>
  <meta charset="utf-8" />
  <title>{{# t }}{{ messages.title }}{{/ t }}</title>
</head>
<body>
  <header>
    <h1>{{# t }}{{ messages.title }}{{/ t }}</h1>
    <p>{{# t }}{{ messages.availableLanguages }}{{/ t }}</p>
    <ul>
      {{# allLangs }}
        <li><a href="index-{{ code }}.html">{{ name }}</a></li>
      {{/ allLangs }}
    </ul>
  </header>
  <div>
    <aside>
      <ul>
        {{# functions }}
          <li><a href="#{{ name }}" title="{{ name }}">{{ name }}</a></li>
        {{/ functions }}
      </ul>
    </aside>
    <section>
      {{# functions }}
        {{> function }}
      {{/ functions }}
    </sectio>
  </div>
</body>
</html>
